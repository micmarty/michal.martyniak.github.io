<%# Aliases %>
<% recent_major_projects = data.projects.major_projects %>

<section class="latest section">
    <div class="section-inner">
        <h2 class="heading">
            <!-- Recent Major Projects Section Title -->
            Recent major projects
        </h2>
        <div class="content">
            <!-- First Recent Major Project -->
            <%= partial project_partial_path('major_project_template'), locals: { project: recent_major_projects[:first_project] } %>
            <hr class="divider" />

            <!-- Second Recent Major Project -->
            <%= partial project_partial_path('major_project_template'), locals: { project: recent_major_projects[:second_project] } %>
        </div>
    </div>
</section>